<div class="container">
    <% myTweets.statuses.forEach( tweets => { %>

     <div class="card" style="width: 18rem;">
    
       <div class="card-body">
         <h5 class="card-title"><%= tweets.text %></h5>
        
      
       </div>
     </div>
    
    
 
   <% }) %>
 </div>



 <div class="container">
    <% myTweets.statuses.forEach( status => { %>
        <% status.entities.hashtags.forEach( hashtag => { %>
     <div class="card" style="width: 18rem;">
    
       <div class="card-body">
         <h5 class="card-title"><%= hashtag.text %></h5>
        
      
       </div>
     </div>
     <% }) %>
    
 
   <% }) %>
 </div>
